{
  "name": "vite-plugin-html-srcset",
  "version": "1.0.0",
  "description": "Vite 7 plugin that processes HTML files and transforms img, picture, and figure tags with srcset attributes",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "keywords": [
    "vite",
    "plugin",
    "html",
    "srcset",
    "responsive-images",
    "image-optimization",
    "vite7"
  ],
  "author": "Claude AI Assistant",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/vite-plugin-html-srcset.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "playground": "cd playground && pnpm run dev",
    "example:basic": "cd examples/basic && pnpm run dev",
    "example:advanced": "cd examples/advanced && pnpm run dev",
    "build:examples": "cd examples && pnpm run -r build",
    "publish:release": "pnpm build && pnpm publish"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "dependencies": {
    "linkedom": "^0.16.10",
    "sharp": "^0.33.2",
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/ui": "^2.1.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vite": "^7.0.0",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.0.0"
}